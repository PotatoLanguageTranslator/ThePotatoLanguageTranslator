<input type=button value="Encryption" onclick="{
   let myTemp = ''
   myString = document.getElementById('myArea01').value.split('')
   for (let myLoop=0;   myLoop < myString.length; myLoop++){
      myTemp = myString[myLoop]

//normal letters
      if (myString[myLoop] == 'a') {myTemp  = 'DASJKLHADFSJKLAFHJADSL'} 
      if (myString[myLoop] == 'b') {myTemp  = 'DFJAKLHDFSALJKFHADSJKFHAJK'} 
      if (myString[myLoop] == 'c') {myTemp  = 'KADSJLFHJHDSFFJADHLJKDA'} 
      if (myString[myLoop] == 'd') {myTemp  = 'FJDKSAHDJKASFHJL'} 
      if (myString[myLoop] == 'e') {myTemp  = 'KDFJLSAHDJFKASHJKDFSJK'} 
      if (myString[myLoop] == 'f') {myTemp  = 'JLADKSHJDFHJAKDSHFADSJK'} 
      if (myString[myLoop] == 'g') {myTemp  = 'KADSJLHFADJKFHAJDSKFHASJK'} 
      if (myString[myLoop] == 'h') {myTemp  = 'JDKSAHJKADSFHADSJKFL'} 
      if (myString[myLoop] == 'i') {myTemp  = 'JASKDHJKLADFHFJKDS'} 
      if (myString[myLoop] == 'j') {myTemp  = 'JDKASFHJASKLDFHASJKFHDLJFK'} 
      if (myString[myLoop] == 'k') {myTemp  = 'DSKAFDASFJKHADSFJK'} 
      if (myString[myLoop] == 'l') {myTemp  = 'DAJKFHADSJKFHASLJK'} 
      if (myString[myLoop] == 'm') {myTemp  = 'JADSFJADSLFHASJK'} 
      if (myString[myLoop] == 'n') {myTemp  = 'JDKASHFJLDKHFSADJKHADSL'} 
      if (myString[myLoop] == 'o') {myTemp  = 'DSJKLAFHDJADSK'} 
      if (myString[myLoop] == 'p') {myTemp  = 'DASJKLFHJAKDSF'} 
      if (myString[myLoop] == 'q') {myTemp  = 'DSAJKJKLDSAHJFKDH'} 
      if (myString[myLoop] == 'r') {myTemp  = 'DHAJLSKFHASJKDFHADSFJ'} 
      if (myString[myLoop] == 's') {myTemp  = 'KADJSHFASJKFHLS'} 
      if (myString[myLoop] == 't') {myTemp  = 'DJKAFLHADSJKFHASL'} 
      if (myString[myLoop] == 'u') {myTemp  = 'FJDSKAFHASJLDKFHASDJK'} 
      if (myString[myLoop] == 'v') {myTemp  = 'DAJKSLFHADJFKSHJL'} 
      if (myString[myLoop] == 'w') {myTemp  = 'DSAJKFHJDSAFKHJ'} 
      if (myString[myLoop] == 'x') {myTemp  = 'JADSKFHLADSFJKL'} 
      if (myString[myLoop] == 'y') {myTemp  = 'DSJKLFHADSJLKFHASDJKLFHASJKL'} 
      if (myString[myLoop] == 'z') {myTemp  = 'JDKLHASJKDFHASDJKFHASJKHADSJKFHADSJKLFHSADJKLFHASFJKAHS'} 
      
// capital letters
      if (myString[myLoop] == 'A') {myTemp  = 'DASJKLHADFSJKLAFHJADSL'} 
      if (myString[myLoop] == 'B') {myTemp  = 'DFJAKLHDFSALJKFHADSJKFHAJK'} 
      if (myString[myLoop] == 'C') {myTemp  = 'KADSJLFHJHDSFFJADHLJKDA'} 
      if (myString[myLoop] == 'D') {myTemp  = 'FJDKSAHDJKASFHJL'} 
      if (myString[myLoop] == 'E') {myTemp  = 'KDFJLSAHDJFKASHJKDFSJK'} 
      if (myString[myLoop] == 'F') {myTemp  = 'JLADKSHJDFHJAKDSHFADSJKL'} 
      if (myString[myLoop] == 'G') {myTemp  = 'KADSJLHFADJKFHAJDSKFHASJKL'} 
      if (myString[myLoop] == 'H') {myTemp  = 'JDKSAHJKADSFHADSJKFL'} 
      if (myString[myLoop] == 'I') {myTemp  = 'JASKDHJKLADFHFJKDS'} 
      if (myString[myLoop] == 'J') {myTemp  = 'JDKASFHJASKLDFHASJKFHDLJFK'} 
      if (myString[myLoop] == 'K') {myTemp  = 'DSKAFDASFJKHADSFJK'} 
      if (myString[myLoop] == 'L') {myTemp  = 'DAJKFHADSJKFHASLJK'} 
      if (myString[myLoop] == 'M') {myTemp  = 'JADSFJADSLFHASJK'} 
      if (myString[myLoop] == 'N') {myTemp  = 'JDKASHFJLDKHFSADJKHADSL'} 
      if (myString[myLoop] == 'O') {myTemp  = 'DSJKLAFHDJADSK'} 
      if (myString[myLoop] == 'P') {myTemp  = 'DASJKLFHJAKDSF'} 
      if (myString[myLoop] == 'Q') {myTemp  = 'DSAJKJKLDSAHJFKDH'} 
      if (myString[myLoop] == 'R') {myTemp  = 'DHAJLSKFHASJKDFHADSFJ'} 
      if (myString[myLoop] == 'S') {myTemp  = 'KADJSHFASJKFHLS'} 
      if (myString[myLoop] == 'T') {myTemp  = 'DJKAFLHADSJKFHASL'} 
      if (myString[myLoop] == 'U') {myTemp  = 'FJDSKAFHASJLDKFHASDJK'} 
      if (myString[myLoop] == 'V') {myTemp  = 'DAJKSLFHADJFKSHJL'} 
      if (myString[myLoop] == 'W') {myTemp  = 'DSAJKFHJDSAFKHJ'} 
      if (myString[myLoop] == 'X') {myTemp  = 'JADSKFHLADSFJKL'} 
      if (myString[myLoop] == 'Y') {myTemp  = 'DSJKLFHADSJLKFHASDJKLFHASJKL'} 
      if (myString[myLoop] == 'Z') {myTemp  = 'JDKLHASJKDFHASDJKFHASJKHADSJKFHADSJKLFHSADJKLFHASFJKAHS'} 

// numbers
      if (myString[myLoop] == '1') {myTemp  = '0'}
      if (myString[myLoop] == '2') {myTemp  = '9'} 
      if (myString[myLoop] == '3') {myTemp  = '8'} 
      if (myString[myLoop] == '4') {myTemp  = '6'} 
      if (myString[myLoop] == '5') {myTemp  = '4'} 
      if (myString[myLoop] == '6') {myTemp  = '3'} 
      if (myString[myLoop] == '7') {myTemp  = '5'} 
      if (myString[myLoop] == '8') {myTemp  = '7'} 
      if (myString[myLoop] == '9') {myTemp  = '2'}
      if (myString[myLoop] == '0') {myTemp  = '1'}
      myString[myLoop] = myTemp
   }
   document.getElementById('myArea02').value = myString.join('')
}"><br><br>



<textarea id="myArea02" rows=4 cols=70></textarea><br> 

<input type=button value="Decryption" onclick="{
   let myTemp1 = ''
   myString1 = myString
   for(let myLoop=0; myLoop < myString1.length; myLoop++){
      myTemp1 = myString1[myLoop]
// decrypting letters
      if (myString1[myLoop] == '_') {myTemp1  = ' '} 
      if (myString1[myLoop] == 'DASJKLHADFSJKLAFHJADSL') {myTemp1  = 'a'} 
      if (myString1[myLoop] == 'DFJAKLHDFSALJKFHADSJKFHAJK') {myTemp1  = 'b'} 
      if (myString1[myLoop] == 'KADSJLFHJHDSFFJADHLJKDA') {myTemp1  = 'c'} 
      if (myString1[myLoop] == 'FJDKSAHDJKASFHJL') {myTemp1  = 'd'} 
      if (myString1[myLoop] == 'KDFJLSAHDJFKASHJKDFSJK') {myTemp1  = 'e'} 
      if (myString1[myLoop] == 'JLADKSHJDFHJAKDSHFADSJKL') {myTemp1  = 'f'} 
      if (myString1[myLoop] == 'KADSJLHFADJKFHAJDSKFHASJKL') {myTemp1  = 'g'} 
      if (myString1[myLoop] == 'JDKSAHJKADSFHADSJKFL') {myTemp1  = 'h'} 
      if (myString1[myLoop] == 'JASKDHJKLADFHFJKDS') {myTemp1  = 'i'} 
      if (myString1[myLoop] == 'JDKASFHJASKLDFHASJKFHDLJFK') {myTemp1  = 'j'} 
      if (myString1[myLoop] == 'DSKAFDASFJKHADSFJK') {myTemp1  = 'k'} 
      if (myString1[myLoop] == 'DAJKFHADSJKFHASLJK') {myTemp1  = 'l'} 
      if (myString1[myLoop] == 'JADSFJADSLFHASJK') {myTemp1  = 'm'} 
      if (myString1[myLoop] == 'JDKASHFJLDKHFSADJKHADSL') {myTemp1  = 'n'} 
      if (myString1[myLoop] == 'DSJKLAFHDJADSK') {myTemp1  = 'o'} 
      if (myString1[myLoop] == 'DASJKLFHJAKDSF') {myTemp1  = 'p'} 
      if (myString1[myLoop] == 'DSAJKJKLDSAHJFKDH') {myTemp1  = 'q'} 
      if (myString1[myLoop] == 'DHAJLSKFHASJKDFHADSFJ') {myTemp1  = 'r'} 
      if (myString1[myLoop] == 'KADJSHFASJKFHLS') {myTemp1  = 's'} 
      if (myString1[myLoop] == 'DJKAFLHADSJKFHASL') {myTemp1  = 't'} 
      if (myString1[myLoop] == 'FJDSKAFHASJLDKFHASDJK') {myTemp1  = 'u'} 
      if (myString1[myLoop] == 'DAJKSLFHADJFKSHJL') {myTemp1  = 'v'} 
      if (myString1[myLoop] == 'DSAJKFHJDSAFKHJ') {myTemp1  = 'w'} 
      if (myString1[myLoop] == 'JADSKFHLADSFJKL') {myTemp1  = 'x'} 
      if (myString1[myLoop] == 'DSJKLFHADSJLKFHASDJKLFHASJKL') {myTemp1  = 'y'} 
      if (myString1[myLoop] == 'JDKLHASJKDFHASDJKFHASJKHADSJKFHADSJKLFHSADJKLFHASFJKAHS') {myTemp1  = 'z'} 
     
 //decrypting numbers
 
      if (myString1[myLoop] == '0') {myTemp1  = '1'}
      if (myString1[myLoop] == '9') {myTemp1  = '2'} 
      if (myString1[myLoop] == '8') {myTemp1  = '3'} 
      if (myString1[myLoop] == '6') {myTemp1  = '4'} 
      if (myString1[myLoop] == '4') {myTemp1  = '5'} 
      if (myString1[myLoop] == '3') {myTemp1  = '6'} 
      if (myString1[myLoop] == '5') {myTemp1  = '7'} 
      if (myString1[myLoop] == '7') {myTemp1  = '8'} 
      if (myString1[myLoop] == '2') {myTemp1  = '9'}
      if (myString1[myLoop] == '1') {myTemp1  = '0'}
    


      myString1[myLoop] = myTemp1 
      document.getElementById('myArea03').value = myString1.join('')
      
}
}"><br><br>
